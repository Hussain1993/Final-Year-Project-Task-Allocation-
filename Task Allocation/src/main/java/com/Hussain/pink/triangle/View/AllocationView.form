<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.Hussain.pink.triangle.View.AllocationView">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="527" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="34b93" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="e94ad" class="javax.swing.JLabel">
            <constraints border-constraint="West"/>
            <properties>
              <text value="Choose an allocation algorithm:"/>
            </properties>
          </component>
          <component id="94e11" class="javax.swing.JComboBox" binding="algorithmBox">
            <constraints border-constraint="Center"/>
            <properties>
              <model>
                <item value="Greedy (Alphabetical)"/>
                <item value="Greedy (Cost)"/>
                <item value="Maximum Allocation"/>
              </model>
            </properties>
          </component>
          <component id="fd45c" class="javax.swing.JButton" binding="doneButton" default-binding="true">
            <constraints border-constraint="East"/>
            <properties>
              <text value="Done"/>
            </properties>
          </component>
          <component id="87e5d" class="javax.swing.JLabel" binding="statusLabel">
            <constraints border-constraint="South"/>
            <properties>
              <foreground color="-16729337"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="c32a9" binding="tablePanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children/>
      </grid>
      <grid id="b3c8c" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="b6936" class="javax.swing.JButton" binding="backButton">
            <constraints/>
            <properties>
              <text value="Back"/>
            </properties>
          </component>
          <component id="2f40b" class="javax.swing.JButton" binding="assignButton" default-binding="true">
            <constraints/>
            <properties>
              <text value="Assign"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
