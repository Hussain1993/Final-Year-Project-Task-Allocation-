          <component id="4f19c" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text value="Manage the tasks here"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="70a6d" binding="tablePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="2830e" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <grid id="167d1" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
                <constraints border-constraint="East"/>
                <properties/>
