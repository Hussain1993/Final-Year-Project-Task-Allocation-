<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.Hussain.pink.triangle.View.TaskManager">
  <grid id="27dc6" binding="root" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="be4a4" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="4f19c" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text value="Manage the tasks here"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="70a6d" binding="tablePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="2830e" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="ab3a6" class="javax.swing.JButton" binding="backButton" default-binding="true">
                <constraints/>
                <properties>
                  <text value="Back"/>
                </properties>
              </component>
              <component id="53f16" class="javax.swing.JButton" binding="updateButton">
                <constraints/>
                <properties>
                  <text value="Update"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
