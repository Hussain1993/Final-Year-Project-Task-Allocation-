                <properties/>
                <border type="none"/>
                <children>
                  <component id="5a93f" class="javax.swing.JLabel" binding="numberOfTasksLabel">
                    <constraints/>
                    <properties>
                      <text value="Number of Tasks: 0"/>
                    </properties>
                  </component>
                </children>
              </grid>
            </children>
          </grid>
        </children>
      </grid>
    </children>
